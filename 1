isim=input("İsminizi girin: ")
soyisim=input("Soyisminizi girin: ")
tc=int(input("Tc girin: "))
Nekadarparakazanıyıor=float(input("Nekadarparakazanıyorsunuz?: "))
yaş=int(input("Yaşınızı girin: "))
sınıf=input("Sınıfınızı girin: ")

if(isim=="zeynep"):
     print=("Bu siteye gelmeyin")
elif(Nekadarparakazanıyıor=="1000"):
     print("Para kazan öyle gel")
elif(yaş=="18"):
     print("Öğrenci kaydınızı yapıldı")
elif(sınıf=="5"):
     print("Büyü oyle gel")          
elif(Nekadarparakazanıyıor=="100000000") :
     print("Kaydınız yapılmıştır") 
elif(isim=="Ayşe" and isim=="Fatma"):
     print("okulumuza hoşgeldiniz")
elif(yaş<=20 or yaş>=60):
     print("Kaydınızı alamadık")
