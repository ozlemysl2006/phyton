kilo=int(input("Kilonuzun giriniz: "))
yaş=input("Yaşınızı giriniz: ")
şehir=input("Nerede yaşıyorsunuz: ")
meyve=input("En sevdiğiniz meyveyi giriniz: ")
tatlı=input("En sevdiğiniz tatlıyı giriniz: ")

if kilo=<100 and kilo=>80 and yaş=>20 and yaş=<30:
    print("Obez")
elif tatlı=="baklava":
    print("433 kaloridir")
elif meyve=="kiraz":
    print("kiraz 69 kaloridir")
elif şehir=="Amasya":
    print("elma yiyiniz")
elif tatlı=="künefe" and  kilo=="85":
    print("şekerli tatlılardan uzak durunuz")
elif şehir=="konya" and meyve=="Ananas":
    print("konyada ananas yetişmez")
elif şehir=="Adana":
    print("Adananın şalgamı meşhurdur")
else:
    print("tekrar deneyiniz")
